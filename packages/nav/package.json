  "peerDependencies": {
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "styled-components": "4.2.0"
  },
  "main": "src/index.js",
  "module": "src/index.module.js",
  "scripts": {
    "preinstall": "npx use-yarn",
    "dev": "rollup -c rollup.config.js -w",
