    "preinstall": "npx use-yarn",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "test": "echo 'No tests, coming soon!'"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "devDependencies": {
    "@caldera-digital/rollup-config": "^0.1.1"
  },
  "peerDependencies": {
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "styled-components": "4.2.0"
  },
