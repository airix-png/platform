{
  "name": "@caldera-digital/theme",
  "version": "0.1.1",
  "description": "Rollup config used by Caldera Digital",
  "keywords": [
    "caldera digital"
  ],
  "homepage": "https://github.com/caldera-digital/platform#readme",
  "bugs": {
    "url": "https://github.com/caldera-digital/platform/issues"
  },
  "repository": {
